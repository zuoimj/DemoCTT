<!DOCTYPE html>
<html lang="vi">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Tuyển dụng</title>
    <link rel="shortcut icon" type="image/png" href="~/Resources/admin/images/favicon.ico" />
    <link href="~/Resources/newhtml/css/select2.css" rel="stylesheet" />
    <link href="~/Resources/newhtml/datatable/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/Resources/newhtml/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Resources/newhtml/css/owl.carousel.min.css" rel="stylesheet">
    <link href="~/Resources/newhtml/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Resources/newhtml/css/main.css" rel="stylesheet">
    <link href="~/Resources/newhtml/css/style-theme.css" rel="stylesheet" />
    <script src='https://www.google.com/recaptcha/api.js'></script>
    <!--[if IE 8]> <html class="no-js lt-ie8"> <![endif]-->
    <!--[if IE 9]> <html class="no-js lt-ie9"> <![endif]-->
    <!--[if !IE]><!-->
    <script>
        if (false) { document.documentElement.className += 'ie10'; }</script><!--<![endif]-->
</head>

<body>
    <!--Header-->
    <div id="header">
        @*<div class="top-header">
              <div class="container">
                  <div class="col-sm-12">
                    <ul class="nav_title clearfix" id="sub-menu02" role="tablist">
                      <li><a href="#">Giới Thiệu</a></li>
                      <li role="presentation"><a href="#">Sản phẩm dịch vụ</a></li>
                      <li role="presentation"><a href="#">Tin tức</a></li>
                      <li role="presentation" class="active"><a href="#">Tuyển dụng</a></li>
                      <li role="presentation"><a href="#">Vnpay</a></li>
                    </ul>
                  </div>
              </div>
            </div>*@
        <div class="cont-header max-992px-hidden">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6"> <a href="@Url.Action("Index","Home")" class="logo"><img src="~/Resources/newhtml/images/logo.png" alt="#" /></a> </div>
                    <div class="col-sm-6">
                        <div class="box-search">
                            <form id="form-search" class="navbar-form navbar-right" role="search" action="~/cong-viec/">
                                <div class="form-group box-table-search">
                                    <input type="text" class="form-control" name="keyword" placeholder="Tìm kiếm tên công việc..." value="">
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--End header-->
    <div class="bg-main menu-main">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <div class="nav_desktop max-992px-hidden">
                        <ul class="clearfix">
                            <li class="active"><a href="@Url.Action("Index","Home")"><i class="fa fa-home"></i></a></li>
                            <li><a target="_blank" href="https://vnpay.vn/ve-chung-toi">Về VNPAY</a></li>
                            <li><a href="@Url.Action("Index","Jobs")">Danh sách công việc</a></li>
                            <li><a target="_blank" href="https://vnpay.vn/hoi-dap">Trợ giúp</a></li>
                        </ul>
                    </div>
                    <!--nav-mobile-->
                    <div class="nav_mobile bg-main max-992px-show">
                        <div class="container">
                            <a href="#" class="btn-menu-main btn-toggle-nav"><i class="fa fa-bars" aria-hidden="true"></i></a>
                            <div class="logo-mobile"><a href="@Url.Action("Index","Home")"><img src="~/Resources/newhtml/images/logo-mobile.png" alt="logo"></a></div>
                            <div class="nav-right">
                                <div class="item-dropdown Foatright dropdown">
                                    <a href="#" class="dropdown-toggle" id="drop_search" data-toggle="dropdown"><i class="fa fa-search"></i> </a>
                                    <div class="dropdown-menu" aria-labelledby="drop_search">
                                        <div class="box-search">
                                            <form class="form-inline">
                                                <button type="submit" class="btn btn-search"><i class="fa fa-search" aria-hidden="true"></i></button><div class="form-group">
                                                    <div class="input-group">
                                                        <input type="text" class="form-control" id="exampleInputAmount" placeholder="Tìm kiếm tên công việc...">
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="menu-mobile cont-toggle-nav">
                            <div id='cssmenu'>
                                <h2 class="title-category">MENU <a href="#" class="btn-toggle-nav btn-close"><i class="fa fa-times" aria-hidden="true"></i></a></h2>
                                <ul class="clearfix">
                                    <li><a href="@Url.Action("Index","Home")">Trang chủ</a></li>
                                    <li><a href="https://vnpay.vn/">Về VNPAY</a></li>
                                    <li><a href="@Url.Action("Index","Jobs")">Danh sách công việc</a></li>
                                    <li><a href="https://vnpay.vn/hoi-dap">Trợ giúp</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <!--end-mobile-->
                </div>
            </div>
        </div>
    </div>

    <!--Start-Index-->
    <!--End Index-->
    @if (TempData["AlertMessage"] != null)
    {
        <div id="alertBox" class="alert @TempData["AlertType"] hide align-center">
            @TempData["AlertMessage"]
        </div>
    }
    @RenderBody()
    <!--Footer-->
    <div id="footer">
        <div class="bottom-footer">
            <div class="container">
                <div class="row">
                    <div class="col-sm-8">
                        <p class="coppyright">Bản quyền ©@DateTime.Now.Year thuộc về Công ty Cổ phần Giải pháp Thanh toán Việt Nam <img src="~/Resources/admin/images/favicon.ico" /></p>
                    </div>
                    <div class="col-sm-4">
                        <div class="social"> <span>Find us:</span> <a target="_blank" href="https://www.facebook.com/TuyendungVNPAY/"><img src="~/Resources/newhtml/images/tuyendung_facebook.png" alt="#" /></a> </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--End Footer-->
    <script type="text/javascript" src="~/Resources/newhtml/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/Resources/newhtml/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Resources/newhtml/datatable/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/Resources/newhtml/js/owl.carousel.js"></script>
    <script type="text/javascript" src="~/Resources/newhtml/js/style-menu.js"></script>
    <script type="text/javascript" src="~/Resources/newhtml/js/jquery.syotimer.js"></script>
    <script type="text/javascript" src="~/Resources/newhtml/js/main.js"></script>
    <script type="text/javascript">
        $(function () {
            $('#alertBox').removeClass('hide');
            $('#alertBox').delay(2000).slideUp(1500);
        });
        $(document).ready(function () {
            $('#table-fill1').dataTable({
                "responsive": true,
            });
        });
    </script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('#slider_carousel').owlCarousel({
                loop: true,
                margin: 30,
                responsiveClass: true,
                responsive: {
                    0: {
                        items: 1,
                        nav: true
                    },
                    600: {
                        items: 3,
                        nav: false
                    },
                    1000: {
                        items: 4,
                        nav: true,
                        loop: false
                    }
                }
            });

        });
    </script>
    <script type="text/javascript">
        var Tawk_API = Tawk_API || {}, Tawk_LoadStart = new Date();
        (function () {
            var s1 = document.createElement("script"), s0 = document.getElementsByTagName("script")[0];
            s1.async = true;
            s1.src = 'https://embed.tawk.to/5a531bee4b401e45400be722/default';
            s1.charset = 'UTF-8';
            s1.setAttribute('crossorigin', '*');
            s0.parentNode.insertBefore(s1, s0);
        })();
    </script>
    @RenderSection("scripts", false)
</body>
</html>
